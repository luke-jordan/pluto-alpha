{
    "aws": {
        "region": "eu-west-1",
        "apiVersion": "2012-08-10"
    },
    "tables": {
        "activeCodes": "ActiveReferralCodes",
        "archivedCodes": "ArchivedReferralCodeTable",
        "userProfile": "UserProfileTable",
        "clientFloatTable": "ClientFloatTable"
    },
    "revocationDefaults": {
        "expiryTimeDays": 7,
        "withdrawalTime": 30,
        "balanceBelow": "10::WHOLE_CURRENCY::ZAR"  
    },
    "userCodeCutOff": {
        "unit": "days",
        "value": 7
    },
    "lambda": {
        "createBoost": "boost_create"
    },
    "publishing": {
        "userEvents": {
            "topicArn": "<referral code topic>"
        }
    }
}