{
    "aws": {
        "region": "us-east-1"
    },
    "db": {
        "user": "friend_api_worker",
        "host": "localhost",
        "database": "jupiter",
        "password": "pwd_for_friends_api",
        "port" :"5430"
    },
    "tables": {
        "accountTable": "account_data.core_account_ledger",
        "friendshipTable": "friend_data.core_friend_relationship",
        "friendRequestTable": "friend_data.friend_request",
        "friendLogTable": "friend_data.friend_log",
        "profileTable": "UserProfileTable",
        "phoneTable": "UserPhoneTable",
        "emailTable": "UserEmailTable"
    },
    "lambdas": {
        "sendOutboundMessages": "outbound_comms_send",
        "calcSavingsHeat": "savings_heat_fetch"
    },
    "cache": {
        "port": 6739,
        "detailsTTL": 900,
        "host": "localhost",
        "keyPrefixes": {
            "profile": "FRIENDS_PROFILE",
            "userId": "FRIENDS_USER_ID",
            "savingsHeat": "SAVINGS_HEAT"
        },
        "ttls": {
            "profile": 25200,
            "userId": 25200
        }
    },
    "templates": {
        "email": {
            "default": {
                "subject": "Jupiter Saving Buddies",
                "templateKey": "emails/friendRequestEmail.html"
            },
            "custom": {
                "subject": "Jupiter Saving Buddies",
                "templateKey": "emails/customFriendRequestEmail.html"
            }
        },
        "sms": {
            "friendRequest": {
                "template": "Greetings. {} has invited you to save some lettuce with them. All things are better done together."
            }
        },
        "blacklist": "\\b(password|pwd)"
    },
    "share": {
        "permissions": ["LAST_ACTIVITY_DATE", "LAST_ACTIVITY_AMOUNT"],
        "userActivities": ["USER_SAVING_EVENT", "WITHDRAWAL", "BOOST_REDEMPTION", "CAPITALIZATION"]
    }
}